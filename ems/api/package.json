{
  "name": "@agrzes/yx-ems-api",
  "version": "0.0.1",
  "description": "",
  "main": "target",
  "scripts": {
    "test": "nyc mocha test/**/*.{ts,tsx}",
    "build": "rm -rf target types && tsc",
    "watch": "tsc --watch --preserveWatchOutput"
  },
  "types": "types",
  "nyc": {
    "all": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "author": "Grzegorz Abramczyk <abramczykg@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "mocha": "^9.1.2",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "source-map-support": "^0.5.20",
    "ts-node": "^10.3.0",
    "typescript": "^4.3.2"
  }
}
